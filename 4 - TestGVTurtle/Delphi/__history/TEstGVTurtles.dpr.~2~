program TEstGVTurtles;

uses
  Vcl.Forms,
  MainForm in 'MainForm.pas' {MainFormGVTurtles} ,
  Vcl.Themes,
  Vcl.Styles;

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  TStyleManager.TrySetStyle('Auric');
  Application.CreateForm(TMainFormGVTurtles, MainFormGVTurtles);
  Application.Run;

end.
