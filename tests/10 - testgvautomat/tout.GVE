tourne2|[.PRC [[[:nbre :motif :cote] [[// forme qui tourne (dans motif)] [repete :nbre [exec ph :motif :cote tg quotient 360 :nbre av 9]]]]]]
Poly12|[.PRC [[[:cote] [[// un polyedre à 12 faces] [polyedre :cote 12 30]]]]]
polyedre|[.PRC [[[:lcote :nbcotes :angle] [[// un polyedre] [bc] [xpolygone :lcote :nbcotes :angle] [td diff 90 :angle] [av :lcote] [tg diff 90 :angle] [polygone :nbcotes :lcote]]]]]
jonction|[.PRC [[[:lcote :angle :anciencap] [[// jonction de deux polygones] [fcap 90] [td diff 90 :angle] [av :lcote] [re :lcote] [fcap :anciencap]]]]]
xpolygone|[.PRC [[[:lcote :nbcotes :angle] [[// xpolygones sur un cercle] [repete :nbcotes [jonction :lcote :angle cap av :lcote td quotient 360 :nbcotes]]]]]]
tourne|[.PRC [[[:n] [[// octogone qui tourne] [repete 36 [octogone :n td 10]]]]]]
octogone|[.PRC [[[:n] [[// un octogone] [polygone 8 :n]]]]]
pentagone|[.PRC [[[:n] [[// un pentagone] [polygone 5 :n]]]]]
hexagone|[.PRC [[[:n] [[// un hexagone] [polygone 6 :n]]]]]
polygone|[.PRC [[[:n :long] [[// un polygone] [repete :n [av :long td quotient 360 :n]]]]]]
carre|[.PRC [[[:n] [[// un carré] [polygone 4 :n]]]]]
triangle|[.PRC [[[:n] [[// un triangle] [polygone 3 :n]]]]]
